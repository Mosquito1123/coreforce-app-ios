# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'hfpower' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
  
  # Pods for hfpower
  #  布局
  pod 'SnapKit'
  #  调试工具，配合Lookin使用（下载地址：https://lookin.work）
  pod 'LookinServer', :subspecs => ['Swift'], :configurations => ['Debug']
  #  分栏 https://github.com/uias/Tabman
  pod "Tabman"
  #  网络 https://github.com/Alamofire/Alamofire
  pod 'Alamofire'
  # 筛选视图 https://github.com/younatics/YNDropDownMenu.git
  pod 'YNDropDownMenu'
  #  网络 https://github.com/Moya/Moya.git
  pod 'Moya'
  #  JSON解析 https://github.com/kakaopensource/KakaJSON.git
  pod 'KakaJSON', '~> 1.1.2'
  #  JSON解析 https://github.com/SwiftyJSON/SwiftyJSON
  pod 'SwiftyJSON'
  #  唤起键盘自动抬高页面 https://github.com/hackiftekhar/IQKeyboardManager
  pod 'IQKeyboardManagerSwift'
  #  图片视频工具类 https://github.com/longitachi/ZLPhotoBrowser
  pod 'ZLPhotoBrowser'
  #  本地存储 https://github.com/sunshinejr/SwiftyUserDefaults
  pod 'SwiftyUserDefaults'
  #  列表下拉刷新、上拉加载（OC） https://github.com/CoderMJLee/MJRefresh
  pod 'MJRefresh'
  pod "ESTabBarController-swift"
  pod 'SVProgressHUD'
  pod 'SDCAlertView'
  pod 'swiftScan'
  pod 'FloatingPanel'
  pod 'FSPagerView'
  pod 'DGCharts'
  pod 'Kingfisher'
  pod 'EmptyStateKit'
  pod 'SwipeCellKit'
end
